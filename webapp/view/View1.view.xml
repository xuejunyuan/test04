<mvc:View
    controllerName="btp.test04.test04.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Shell id="shell">
        <App id="app">
            <pages>
                <Page id="page" title="{i18n>title}">
                    <content>
                        <Table items="{/zsflightSet}" noDataText="Drop column list items here and columns in the area above" id="table0" xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" cd:w5g.dt.context="/zsflightSet">
                            <items>
                                <ColumnListItem type="Active" id="item0">
                                    <cells>
                                        <Text text="{Carrid}" id="text2"/>
                                        <Text text="{Connid}" id="text3"/>
                                        <Text text="{Planetype}" id="text4"/></cells>
                                </ColumnListItem>
                            </items>
                            <columns>
                                <Column id="column0">
                                    <header>
                                        <Label text="Carrid" id="label0"/>
                                    </header>
                                </Column>
                                <Column id="column1">
                                    <header>
                                        <Label text="Connid" id="label1"/>
                                    </header>
                                </Column>
                                <Column id="column2">
                                    <header>
                                        <Label text="Planetype" id="label2"/>
                                    </header>
                                </Column></columns></Table>
                    </content>
                </Page>
            </pages>
        </App>
    </Shell>
</mvc:View>
